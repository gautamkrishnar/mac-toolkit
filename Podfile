# Uncomment the next line to define a global platform for your project
platform :osx, '10.12'

target 'Mac Toolkit' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Mac Toolkit
  pod 'AppCenter'

  target 'Mac ToolkitTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Mac ToolkitUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
